{
  "name": "liquid-web",
  "version": "0.0.96",
  "description": "React-native-web client to connect to api.liquid.vote",
  "author": "github.com/liquidvote",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --port 3000 --progress --colors --inline --host 0.0.0.0",
    "start:prod": "API_URL_V1=https://api.liquid.vote API_URL_V2=https://pure-api.liquid.vote/v2 npm start",
    "build": "webpack --progress --colors",
    "build:prod": "rm -rf dist && API_URL_V1=https://api.liquid.vote API_URL_V2=https://pure-api.liquid.vote/v2 NODE_ENV=production webpack --progress --colors",
    "serve:prod": "API_URL_V1=https://api.liquid.vote API_URL_V2=https://pure-api.liquid.vote/v2 NODE_ENV=production serve -s dist",
    "predeploy": "npm version patch",
    "deploy": "git checkout gh-pages && git merge master --no-edit && npm run build && git commit -am 'Update bundle.js' && git push && git checkout master",
    "postdeploy": "git push --follow-tags",
    "heroku-postbuild": "npm run build:prod",
    "test": "eslint . --cache && npm run build"
  },
  "dependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "7",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.5",
    "babel-preset-env": "^1.3.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-app": "^3.0.2",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "dateformat": "^2.0.0",
    "deep-equal": "^1.0.1",
    "es6-promise": "^4.1.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-react-filenames": "dsernst/eslint-plugin-react-filenames",
    "exports-loader": "^0.6.4",
    "file-loader": "^0.11.1",
    "html-webpack-include-assets-plugin": "0.0.7",
    "html-webpack-plugin": "^2.30.1",
    "imports-loader": "^0.7.1",
    "letter-grade": "^1.0.0",
    "localforage": "^1.5.0",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-aria-live": "^1.0.3",
    "react-burger-menu": "^2.1.4",
    "react-collapse": "^4.0.1",
    "react-dom": "^15.6.1",
    "react-icons": "^2.2.3",
    "react-native-aws3": "^0.0.8",
    "react-native-vector-icons": "^4.0.1",
    "react-native-web": "0.0.120",
    "react-parallax": "^1.4.4",
    "react-redux": "^5.0.3",
    "react-responsive": "^1.2.10",
    "react-router-dom": "^4.1.1",
    "react-tappable": "^1.0.2",
    "react-visibility-sensor": "^3.10.1",
    "redux": "^3.6.0",
    "redux-persist": "^4.6.0",
    "remote-redux-devtools": "^0.5.7",
    "serve": "^6.0.6",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2",
    "whatwg-fetch": "^2.0.3"
  }
}
